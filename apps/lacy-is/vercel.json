{
    "buildCommand": "pnpm with-env next build",
    "ignoreCommand": "pnpm dlx turbo-ignore",
    "cleanUrls": true,
    "trailingSlash": false,
    "rewrites": [
        {
            "source": "/lacy/",
            "destination": "https://lacymorrow.com"
        },
        {
            "source": "/lacy/:match*",
            "destination": "https://lacymorrow.com/:match*"
        },
        {
            "source": "/",
            "destination": "https://lacy.is/"
        },
        {
            "source": "/:match*",
            "destination": "https://lacy.is/:match*"
        }
    ]
}