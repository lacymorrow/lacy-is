{
    "cleanUrls": true,
    "trailingSlash": false,
    "rewrites": [
        {
            "source": "/",
            "destination": "https://lacy-is.vercel.app/"
        },
        {
            "source": "/:match*",
            "destination": "https://lacy-is.vercel.app/:match*"
        }
    ]
}